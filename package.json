{
  "name": "@ctrl/magnet-link",
  "version": "0.0.0-placeholder",
  "description": "Parse a magnet URI into an object",
  "author": "Scott Cooper <scttcper@gmail.com>",
  "homepage": "https://magnet-link.vercel.app",
  "license": "MIT",
  "repository": "scttcper/magnet-link",
  "keywords": [
    "torrent",
    "magnet",
    "link",
    "magnet-uri"
  ],
  "main": "./dist/index.js",
  "module": "./dist/module/index.js",
  "typings": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build:demo": "rollup -c rollup.demo.js",
    "watch:demo": "rollup -c rollup.demo.js -w",
    "lint": "eslint --ext .js,.ts, .",
    "lint:fix": "eslint --fix --ext .js,.ts, .",
    "prepare": "npm run build",
    "build": "tsc -p tsconfig.build.json && tsc -p tsconfig.module.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --coverage --no-cache"
  },
  "dependencies": {
    "@ctrl/ts-base32": "^1.2.2"
  },
  "devDependencies": {
    "@ctrl/eslint-config": "1.0.2",
    "@jest/globals": "26.1.0",
    "@rollup/plugin-node-resolve": "8.1.0",
    "@types/node": "14.0.14",
    "buffer": "5.6.0",
    "jest": "26.1.0",
    "rollup-plugin-livereload": "1.3.0",
    "rollup-plugin-node-builtins": "2.1.2",
    "rollup-plugin-node-globals": "1.4.0",
    "rollup-plugin-serve": "1.0.1",
    "rollup-plugin-terser": "6.1.0",
    "rollup-plugin-typescript2": "0.27.1",
    "rollup": "2.19.0",
    "ts-jest": "26.1.1",
    "typescript": "3.9.6"
  },
  "release": {
    "branch": "master"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  },
  "engines": {
    "node": ">=10.19.0"
  }
}
