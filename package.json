{
  "name": "@ctrl/magnet-link",
  "version": "0.0.0-placeholder",
  "description": "Parse a magnet URI into an object",
  "author": "Scott Cooper <scttcper@gmail.com>",
  "homepage": "https://magnet-link.vercel.app",
  "license": "MIT",
  "repository": "scttcper/magnet-link",
  "keywords": [
    "torrent",
    "magnet",
    "link",
    "magnet-uri"
  ],
  "main": "dist/index.js",
  "module": "dist/module/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build:demo": "rollup -c rollup.demo.js",
    "watch:demo": "rollup -c rollup.demo.js -w",
    "lint": "eslint --ext .js,.ts, .",
    "lint:fix": "eslint --fix --ext .js,.ts, .",
    "prepare": "npm run build",
    "build": "tsc -p tsconfig.build.json && tsc -p tsconfig.module.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --coverage --no-cache"
  },
  "dependencies": {
    "@ctrl/ts-base32": "^1.2.6"
  },
  "devDependencies": {
    "@babel/core": "7.13.16",
    "@babel/preset-env": "7.13.15",
    "@babel/preset-typescript": "7.13.0",
    "@ctrl/eslint-config": "2.0.4",
    "@jest/globals": "27.0.0-next.8",
    "@rollup/plugin-node-resolve": "11.2.1",
    "@types/node": "14.14.41",
    "babel-jest": "27.0.0-next.8",
    "buffer": "6.0.3",
    "jest": "27.0.0-next.8",
    "rollup": "2.45.2",
    "rollup-plugin-livereload": "2.0.0",
    "rollup-plugin-node-builtins": "2.1.2",
    "rollup-plugin-node-globals": "1.4.0",
    "rollup-plugin-serve": "1.1.0",
    "rollup-plugin-terser": "7.0.2",
    "rollup-plugin-typescript2": "0.30.0",
    "typescript": "4.2.4"
  },
  "release": {
    "branch": "master"
  },
  "jest": {
    "coverageProvider": "v8"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ],
      "@babel/preset-typescript"
    ]
  },
  "engines": {
    "node": ">=10.19.0"
  },
  "volta": {
    "node": "16.0.0"
  }
}
