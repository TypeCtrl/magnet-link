{
  "name": "@ctrl/magnet-link",
  "version": "0.0.0-placeholder",
  "description": "Parse a magnet URI into an object",
  "author": "Scott Cooper <scttcper@gmail.com>",
  "license": "MIT",
  "repository": "",
  "keywords": [
    "torrent",
    "magnet",
    "link",
    "magnet-uri"
  ],
  "source": "src/index.ts",
  "main": "./magnet-tr.js",
  "module": "./magnet-tr.m.js",
  "typings": "./src/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "demo": "npm-run-all --parallel start:demo watch:demo",
    "build:demo": "rollup -c rollup.demo.js",
    "watch:demo": "rollup -c rollup.demo.js -w",
    "start:demo": "serve demo/public",
    "lint": "tslint -t stylish -p .",
    "lint:fix": "tslint -p tsconfig.json -t stylish --fix 'src/**/*.ts' 'test/**/*.ts'",
    "prebuild": "rimraf dist",
    "build": "microbundle build --target node --format cjs,es -o dist",
    "postbuild": "cp package.json dist && cp LICENSE dist && cp README.md dist && rimraf dist/test && rimraf dist/demo",
    "build:docs": "typedoc --out docs --target es6 --theme minimal --mode file src && touch docs/.nojekyll",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --coverage --no-cache",
    "semantic-release": "cd dist && semantic-release"
  },
  "dependencies": {
    "@ctrl/ts-base32": "^1.0.4"
  },
  "devDependencies": {
    "@types/jest": "23.3.13",
    "@types/node": "10.12.18",
    "@types/query-string": "6.2.0",
    "buffer": "5.2.1",
    "jest": "23.6.0",
    "microbundle": "0.9.0",
    "npm-run-all": "4.1.5",
    "prettier": "1.15.3",
    "rimraf": "2.6.3",
    "rollup": "1.0.2",
    "rollup-plugin-node-builtins": "2.1.2",
    "rollup-plugin-node-globals": "1.4.0",
    "rollup-plugin-node-resolve": "4.0.0",
    "rollup-plugin-typescript2": "0.18.1",
    "semantic-release": "15.13.2",
    "serve": "10.1.1",
    "ts-jest": "23.10.5",
    "ts-node": "7.0.1",
    "tslint": "5.12.0",
    "tslint-config-prettier": "1.17.0",
    "tslint-xo": "0.11.0",
    "typescript": "3.2.2"
  },
  "release": {
    "branch": "master"
  },
  "engines": {
    "node": ">=8.0.0"
  }
}
