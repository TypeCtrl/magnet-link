{
  "name": "@ctrl/magnet-link",
  "version": "0.0.0-placeholder",
  "description": "Parse a magnet URI into an object",
  "author": "Scott Cooper <scttcper@gmail.com>",
  "license": "MIT",
  "repository": "TypeCtrl/magnet-link",
  "keywords": [
    "torrent",
    "magnet",
    "link",
    "magnet-uri"
  ],
  "main": "./dist/index.js",
  "module": "./dist/module/index.js",
  "typings": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build:demo": "rollup -c rollup.demo.js",
    "watch:demo": "rollup -c rollup.demo.js -w",
    "lint": "eslint --ext .js,.ts, .",
    "lint:fix": "eslint --fix --ext .js,.ts, .",
    "prepare": "npm run build",
    "build": "tsc -p tsconfig.build.json && tsc -p tsconfig.module.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --coverage --no-cache",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "@ctrl/ts-base32": "^1.1.4"
  },
  "devDependencies": {
    "@types/jest": "25.1.3",
    "@types/node": "13.7.0",
    "@typescript-eslint/eslint-plugin": "2.19.0",
    "@typescript-eslint/parser": "2.19.0",
    "buffer": "5.4.3",
    "eslint": "6.8.0",
    "eslint-config-xo-space": "0.24.0",
    "eslint-config-xo-typescript": "0.26.0",
    "eslint-plugin-import": "2.20.1",
    "jest": "25.1.0",
    "rollup": "1.32.0",
    "rollup-plugin-livereload": "1.0.4",
    "rollup-plugin-node-builtins": "2.1.2",
    "rollup-plugin-node-globals": "1.4.0",
    "@rollup/plugin-node-resolve": "7.1.1",
    "rollup-plugin-serve": "1.0.1",
    "rollup-plugin-terser": "5.2.0",
    "rollup-plugin-typescript2": "0.25.3",
    "semantic-release": "17.0.2",
    "ts-jest": "25.2.0",
    "typescript": "3.7.5"
  },
  "release": {
    "branch": "master"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  },
  "engines": {
    "node": ">=8.0.0"
  }
}
