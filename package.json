{
  "name": "@ctrl/magnet-link",
  "version": "0.0.0-placeholder",
  "description": "Parse a magnet URI into an object",
  "author": "Scott Cooper <scttcper@gmail.com>",
  "homepage": "https://magnet-link.vercel.app",
  "license": "MIT",
  "repository": "scttcper/magnet-link",
  "keywords": [
    "torrent",
    "magnet",
    "link",
    "magnet-uri"
  ],
  "type": "module",
  "exports": "./index.js",
  "types": "./index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "demo:build": "rollup -c rollup.demo.js",
    "demo:watch": "rollup -c rollup.demo.js -w",
    "lint": "eslint --ext .js,.ts, .",
    "lint:fix": "eslint --fix --ext .js,.ts, .",
    "prepare": "npm run build",
    "build": "tsc -p tsconfig.build.json",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "npm run test -- --watch",
    "test:ci": "npm run test -- --ci --reporters=default --reporters=jest-junit --coverage"
  },
  "dependencies": {
    "@ctrl/ts-base32": "^2.0.1"
  },
  "devDependencies": {
    "@ctrl/eslint-config": "2.0.6",
    "@jest/globals": "27.0.1",
    "@rollup/plugin-node-resolve": "13.0.0",
    "@sindresorhus/tsconfig": "1.0.2",
    "@types/node": "15.6.1",
    "buffer": "6.0.3",
    "jest": "27.0.1",
    "jest-junit": "12.1.0",
    "rollup": "2.50.3",
    "rollup-plugin-livereload": "2.0.0",
    "rollup-plugin-node-builtins": "2.1.2",
    "rollup-plugin-node-globals": "1.4.0",
    "rollup-plugin-serve": "1.1.0",
    "rollup-plugin-terser": "7.0.2",
    "rollup-plugin-typescript2": "0.30.0",
    "ts-jest": "27.0.1",
    "typescript": "4.3.2"
  },
  "release": {
    "branch": "master"
  },
  "jest": {
    "preset": "ts-jest/presets/default-esm",
    "globals": {
      "ts-jest": {
        "useESM": true
      }
    }
  },
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  }
}
